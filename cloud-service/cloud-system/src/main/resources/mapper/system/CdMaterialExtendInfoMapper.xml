<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.system.mapper.CdMaterialExtendInfoMapper">

    <resultMap type="CdMaterialExtendInfo" id="CdMaterialExtendInfoResult">
        <result property="id" column="id"/>
        <result property="materialCode" column="material_code"/>
        <result property="materialDesc" column="material_desc"/>
        <result property="customerName" column="customer_name"/>
        <result property="productType" column="product_type"/>
        <result property="lifeCycle" column="life_cycle"/>
        <result property="isPuttingOut" column="is_putting_out"/>
        <result property="isZnAttestation" column="is_zn_attestation"/>
        <result property="establishDate" column="establish_date"/>
        <result property="delFlag" column="del_flag"/>
        <result property="remark" column="remark"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

    <sql id="selectCdMaterialExtendInfoVo">
        select id,
               material_code,
               material_desc,
               customer_name,
               product_type,
               life_cycle,
               is_putting_out,
               is_zn_attestation,
               establish_date,
               del_flag,
               remark,
               create_time,
               create_by,
               update_time,
               update_by
        from cd_material_extend_info
    </sql>


</mapper>
