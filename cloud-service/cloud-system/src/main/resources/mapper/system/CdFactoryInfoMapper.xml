<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.system.mapper.CdFactoryInfoMapper">

    <resultMap type="CdFactoryInfo" id="CdFactoryInfoResult">
        <result property="id" column="id"/>
        <result property="companyCodeV" column="company_code_v"/>
        <result property="companyCode" column="company_code"/>
        <result property="companyDesc" column="company_desc"/>
        <result property="factoryCode" column="factory_code"/>
        <result property="purchaseOrg" column="purchase_org"/>
        <result property="delFlag" column="del_flag"/>
        <result property="remarks" column="remarks"/>
        <result property="createTime" column="create_time"/>
        <result property="createBy" column="create_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

    <sql id="selectCdFactoryInfoVo">
        select id,
               company_code_v,
               company_code,
               company_desc,
               factory_code,
               purchase_org,
               del_flag,
               remarks,
               create_time,
               create_by,
               update_time,
               update_by
        from cd_factory_info
    </sql>


</mapper>
